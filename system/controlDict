FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

application     foamRun;

solver          multicomponentFluid;

startFrom       latestTime;

//startTime       0;

stopAt          endTime;
endTime         0.0010;

deltaT          1e-9;

adjustTimeStep  yes;

maxCo           1;

maxDeltaT       1e-7;

writeControl    adjustableRunTime;

writeInterval   1e-6;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;
