FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         cellLimited Gauss linear 1;
}

divSchemes

{
    default         none;
    div(phi,U)      Gauss upwind; //vanLeer;               
    div(phi,Yi_h)   Gauss upwind; //vanLeer;               
    div(phi,K)      Gauss upwind; //vanLeer; 
    div(phid,p)     Gauss upwind; //vanLeer; 
    div(phi,epsilon) Gauss upwind; //vanLeer; 
    div(phi,k)      Gauss upwind; //vanLeer; 
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

// ************************************************************************* //
