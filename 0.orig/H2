FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];
internalField   uniform 0;

boundaryField
{
    airinlet
    {
        type            fixedValue;
        value           uniform 0;      
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    plate
    {
        type            zeroGradient;
    }

    jetinlet
    {
        type            fixedValue;
        value           uniform 1;      
    }

    farfield
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

