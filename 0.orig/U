FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1000 0 0);

boundaryField
{
    airinlet
    {
        type            fixedValue;
        value           uniform (1000 0 0);
    }

    jetinlet
    {
        type            fixedValue;
        value           uniform (0 340 0);
    }

    outlet         {type zeroGradient;}

    farfield       {type zeroGradient;}

    plate          {type noSlip;}

    frontAndBackPlanes {type empty;}

}
